  "scripts": {
    "dev": "cross-env nodemon src/server.ts",
    "staging": "cross-env NODE_ENV=staging nodemon src/server.ts",
    "test": "cross-env NODE_ENV=test jest --watch --i",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk node_modules/.bin/jest --runInBand --watch",
    "debug": "cross-env SLS_DEBUG=* node --inspect-brk ./node_modules/.bin/serverless offline start -t --env local",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage -i",
    "make:migration": "./scripts/create_migration.sh",
    "make:model": "./scripts/create_model.sh",
    "make:controller": "./scripts/create_controller.sh",
    "make:validator": "./scripts/create_validator.sh",
    "migrations:run": "./scripts/migrate.sh",
    "migrations:copy": "./scripts/copy_migrations.sh",
    "seeds:copy": "./scripts/copy_seeds.sh",
    "database:reset": "yarn sequelize db:migrate:undo:all && yarn migrations:run && yarn sequelize db:seed:all",
    "docker:db:migrate": "docker-compose exec server bash ./scripts/migrate.sh",
    "docker:db:migrate:undo": "docker-compose exec server yarn sequelize db:migrate:undo",
    "docker:db:seed": "docker-compose exec server yarn sequelize db:seed:all",
    "docker:db:reset": "docker-compose exec server yarn sequelize db:migrate:undo:all && docker-compose exec server bash ./scripts/migrate.sh && docker-compose exec server yarn sequelize db:seed:all"
  },
